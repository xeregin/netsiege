#mysqlslap --concurrency=200 --iterations=20000 --create-schema=test --query="INSERT INTO random_people SELECT str_random('Cc{3}c(4)') AS firstname ,str_random('Cc{5}c(6)') AS lastname, str_random('Cc{5}[street|lane|road|park] d{1}d(2)') AS adress, str_random('Cc{5}c(6)') AS city ,str_random('d{5}-d{4}') AS zip, str_random('[0011|+1|+61|007] d{8}') AS phone, str_random('[m|f]') AS sex;"

sudo su root -c "mysqlslap --concurrency=200 --iterations=20000 --create-schema=test --query=\"INSERT INTO random_people SELECT str_random('Cc{3}c(4)') AS firstname ,str_random('Cc{5}c(6)') AS lastname, str_random('Cc{5}[street|lane|road|park] d{1}d(2)') AS adress, str_random('Cc{5}c(6)') AS city ,str_random('d{5}-d{4}') AS zip, str_random('[0011|+1|+61|007] d{8}') AS phone, str_random('[m|f]') AS sex;\""
