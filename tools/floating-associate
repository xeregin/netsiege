for i in {1..10}; do
  eval $(printf "neutron floatingip-associate ";
         printf "`neutron floatingip-list | awk '/10.127.102/ {print $2}'`" | awk "FNR == $i" | tr '\n' ' ' ;
         printf " `neutron port-list | grep $(nova list | grep ACTIVE | sed 's/.*=//' | sed 's/ .*//' | awk "FNR == $i") | awk '{print $2}'`");
done
